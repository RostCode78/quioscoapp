generator client {
  provider = "prisma-client-js"
}

datasource db {
  // provider : Define la base de datos que vamos a utilizar
  // Despues de cambiar el provider aqu√≠ es necesario cambiarlo tambien en .ENV
  provider = "mysql" 
  url      = env("DATABASE_URL")
}
